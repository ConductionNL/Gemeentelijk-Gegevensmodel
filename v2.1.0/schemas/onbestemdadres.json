{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "OnbestemdAdres",
  "description": "Represents a OnbestemdAdres in the system",
  "properties": {
    "huisletter": {
      "description": "Property huisletter",
      "type": "string"
    },
    "huisnummer": {
      "description": "Property huisnummer",
      "type": "string"
    },
    "huisnummertoevoeging": {
      "description": "Property huisnummertoevoeging",
      "type": "string"
    },
    "postcode": {
      "description": "Property postcode",
      "type": "string",
      "pattern": "^[1-9][0-9]{3}[A-Z]{2}$"
    },
    "straatnaam": {
      "description": "Property straatnaam",
      "type": "string"
    }
  }
}